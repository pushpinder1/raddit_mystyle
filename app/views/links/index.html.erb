

<div class="container">
<p id="notice"><%= notice %></p>
<p id="heading">Listing Links</p>
<hr/>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Ur</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |link| %>
      <tr>
        <td><%= link.title %></td>
        <td><%= link.ur %></td>
        <td><%= link_to 'Show', link %>
        <% if link.user == current_user %>
            <%= link_to 'Edit', edit_link_path(link) %>
            <%= link_to 'Destroy', @link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>